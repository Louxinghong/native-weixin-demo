<view>
  <vCollapse value="{{ activeNames }}" bind:change="onChange" accordion>
    <vCollapseItem title="设备信息" name="1">
      <view>手机型号：{{ systemInfo.model }} </view>
      <view>设备像素比：{{ systemInfo.pixelRatio }}</view>
      <view>窗口宽度：{{ systemInfo.windowWidth }}</view>
      <view>窗口高度：{{ systemInfo.windowHeight }}</view>
      <view>语言：{{ systemInfo.language }}</view>
      <view>微信版本号：{{ systemInfo.version }}</view>
      <view>操作系统版本号：{{ systemInfo.system }}</view>
      <view>客户端平台：{{ systemInfo.platform }}</view>
    </vCollapseItem>
  </vCollapse>
</view>
